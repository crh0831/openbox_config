# Startup Conky after a delay
~/conky.sh &

# A Launcher
/usr/bin/synapse &

# Guake-like Terminal
tilda &

# Xscreensaver so I can run XJack!
xscreensaver -no-splash &

# Kill Capslock Key
setxkbmap -option ctrl:nocaps &

