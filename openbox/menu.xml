<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Terminal">
			<action name="Execute">
				<execute>terminator</execute>
			</action>
		</item>
		<item label="Google Chrome">
			<action name="Execute">
				<execute>google-chrome</execute>
			</action>
		</item>
		<item label="File Manager">
			<action name="Execute">
				<execute>thunar</execute>
			</action>
		</item>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="Multiple Monitors">
			<action name="Execute">
				<execute>arandr</execute>
			</action>
		</item>
		<item label="LXAppearance">
			<action name="Execute">
				<execute>/usr/bin/lxappearance</execute>
			</action>
		</item>
		<item label="Nitrogen">
			<action name="Execute">
				<execute>nitrogen</execute>
			</action>
		</item>
		<item label="ObAutostart">
			<action name="Execute">
				<execute>gedit ~/.config/openbox/autostart</execute>
			</action>
		</item>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<item label="ObKey">
			<action name="Execute">
				<execute>~/.config/openbox/obkey/obkey</execute>
			</action>
		</item>
		<item label="ObMenu">
			<action name="Execute">
				<execute>obmenu</execute>
			</action>
		</item>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Screensaver">
			<action name="Execute">
				<execute>xscreensaver-demo</execute>
			</action>
		</item>
		<separator/>
		<item label="Logout">
			<action name="Execute">
				<execute>openbox --exit</execute>
			</action>
		</item>
		<item label="Reboot">
			<action name="Execute">
				<execute>/usr/sbin/shutdown -r now</execute>
			</action>
		</item>
		<item label="Shutdown">
			<action name="Execute">
				<execute>/usr/sbin/shutdown -h now</execute>
			</action>
		</item>
	</menu>
</openbox_menu>
